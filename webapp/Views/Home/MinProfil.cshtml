@model webapp.Models.ProfileViewModel

<h2>Din profil.</h2>
<hr />

@Html.Partial("ProfilPartial", Model)

<div class="bold">
    Dina vänförfrågningar
</div>


    @foreach (var förfrågan in Model.FriendRequests)
    {
        <div>
            <div class="bold mt10">
                @förfrågan.Avsändare
                @Html.ActionLink("Ja", "AcceptFriendRequest", "FriendRequest", new { @friendRequestId = förfrågan.Id, }, new {  @class = "btn btn-success" })
                @*<input type="submit" class="btn btn-success" id="acceptera" value="Ja" />*@
                @Html.ActionLink("Nej", "DeclineFriendRequest", förfrågan.Id, new { @class = "btn btn-danger" })

                @*<input type="submit" class="btn btn-danger" id="neka" value="Nej" />*@
            </div>


        </div>

    }
